<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.linxz.main">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mvp.appui.AppLauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvp.appui.LauncherScrollActivity"/>
        <activity android:name=".mvp.appui.MainActivity"/>
        <activity android:name=".mvp.appui.LoadFaildActivity"/>
        <activity android:name=".mvp.appui.SettingActivity"/>
    </application>

</manifest>
